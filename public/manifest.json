{ 
   "name": "Chrome React Extension", 
   "description": "使用 React TypeScript 构建 Chrome 扩展", 
   "version": "1.1", 
   "manifest_version": 3, 
   "action": { 
       "default_popup": "index.html", 
       "default_title": "Open the popup" 
   },
   "background": {
    "service_worker": "background.js"
   }, 
   "icons": { 
       "16": "logo192.png", 
       "48": "logo192.png", 
       "128": "logo192.png" 
   },
   "permissions":["notifications","storage"]
} 